<Window x:Class="Editor.NewForms.NewInteractable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="New Interactable" Height="327" Width="488" KeyDown="Window_KeyUp_1">
    <Grid>
        <TextBlock Margin="50,30">Please enter a name for the new Interactable:</TextBlock>
        <TextBox Name="txtName" Height="25" Margin="50,70,50,190"></TextBox>
        <TextBlock  Height="30" Margin="50,130,50,139">Please enter the group name:</TextBlock>
        <TextBox Name="txtGroup" Height="20" Margin="50,166,50,103"></TextBox>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="86,180,116,20" RenderTransformOrigin="0.304,0.5">
            <Button Margin="20" Width="100" Height="25" Click="Button_Click_1">OK</Button>
            <Button Margin="20" Width="100" Height="25" Click="Button_Click_2">Cancel</Button>
        </StackPanel>
    </Grid>
</Window>
