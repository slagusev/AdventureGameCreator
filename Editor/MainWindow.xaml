<Window x:Class="Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="125" Width="525" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Button Name="btnNew" Height="30" Margin="5" Grid.Column="1" Click="btnNew_Click">New</Button>
            <Button Name="btnLoad" Height="30" Margin="5" Grid.Column="2" Click="btnLoad_Click">Load</Button>
            <Button Name="btnExit" Height="30" Margin="5" Grid.Column="3" Click="btnExit_Click">Exit</Button>
        </Grid>
    </Grid>
</Window>
